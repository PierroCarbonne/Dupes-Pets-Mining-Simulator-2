          "type": "panel",
          "id": "ccLibrariesPanel",
          "label": "$$$/ccLibrariesPanel/DefaultPanelTitle=Libraries"
        },
        {
          "type": "panel",
          "id": "ccLibrariesManager",
          "label": "$$$/ccLibrariesManager/DefaultPanelTitle=CC Libraries Manager"
        }
      ]
    },
    {
      "app": "PS",
      "minVersion": "22.5.0",
      "entrypoints": [
        {
          "type": "panel",
          "id": "ccLibrariesPanel",
          "label": "$$$/ccLibrariesPanel/DefaultPanelTitle=Libraries",
          "minimumSize": {
            "width": 245,
            "height": 165
          },
          "maximumSize": {
            "width": 7680,
            "height": 7680
          },
          "preferredDockedSize": {
            "width": 245,
            "height": 300
          },
          "preferredFloatingSize": {
            "width": 245,
            "height": 300
          }
        },
        {
          "type": "command",
          "id": "adobeStockLicensingPlugin",
          "label": "$$$/stockLicensePanel/DefaultPanelTitle=Stock"
        },
        {
          "type": "command",
          "id": "ccLibrariesManager",
          "label": "$$$/ccLibrariesManager/DefaultPanelTitle=CC Libraries Manager"
        }
      ]
    }
  ],
  "icons": [
    {
      "width": 23,
      "height": 23,
      "path": "images/IconDark.png",
      "theme": [
        "darkest",
        "dark"
