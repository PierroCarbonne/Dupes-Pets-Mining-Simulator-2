    },
    {
      "width": 23,
      "height": 23,
      "path": "images/IconLight.png",
      "theme": [
        "lightest",
        "light"
      ],
      "scale": [
        1,
        2
      ]
    }
  ],
  "entrypoints": [
    {
      "type": "panel",
      "id": "ccLibrariesPanel",
      "label": "$$$/ccLibrariesPanel/DefaultPanelTitle=Libraries",
      "minimumSize": {
        "width": 245,
        "height": 165
      },
      "maximumSize": {
        "width": 7680,
        "height": 7680
